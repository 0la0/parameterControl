<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <etc.a0la0.osccontroller.app.ui.parameterspace.views.EditSpaceView
        android:id="@+id/editSpaceView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/black"/>

    <RelativeLayout
        android:id="@+id/iconContainer"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/tiltIcon"
            android:layout_width="@dimen/space_preset_icon_size"
            android:layout_height="@dimen/space_preset_icon_size"
            android:tint="@color/white"
            android:src="@drawable/ic_circle_filled"/>

    </RelativeLayout>

</RelativeLayout>
