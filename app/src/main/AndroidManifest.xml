<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="etc.a0la0.osccontroller">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".app.ui.home.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity
            android:name=".app.ui.edit.EditActivity"
            android:label="@string/edit"
            android:parentActivityName=".app.ui.home.MainActivity" />

        <activity
            android:name=".app.ui.setup.SetupActivity"
            android:label="@string/setup"
            android:parentActivityName=".app.ui.home.MainActivity" />

        <activity
            android:name=".app.ui.parameterspace.SpaceActivity"
            android:label="@string/edit_parameter_space"
            android:parentActivityName=".app.ui.home.MainActivity" />

        <activity
            android:name=".app.ui.parameterspaceplay.SpacePlayActivity"
            android:label="@string/play_parameter_space"
            android:parentActivityName=".app.ui.home.MainActivity" />

    </application>

</manifest>